{
  "name": "greenhouse-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "ts-node --project tsconfig.orm.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:show": "yarn typeorm migration:show -d src/db/dataSource.ts",
    "migration:generate": "yarn typeorm migration:generate -d src/db/dataSource.ts",
    "migration:create": "yarn typeorm migration:create",
    "migration:run": "yarn typeorm migration:run -d ./src/db/dataSource.ts",
    "migration:revert": "yarn typeorm migration:revert -d src/db/dataSource.ts"
  },
